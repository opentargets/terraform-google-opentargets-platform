// --- Open Targets Platform PERMANENT development deployment ---//
// --- Release information --- //
config_release_name = "mbdevpf"

// --- Deployment configuration --- //
config_deployment_regions = ["europe-west1"]

// --- Elastic Search configuration --- //
config_vm_elastic_search_data_volume_image = "platform-20230622-1643-es"

// --- Clickhouse configuration --- //
config_vm_clickhouse_data_volume_image = "platform-20230622-1643-ch"

// --- API configuration --- //
config_vm_platform_api_image_version = "dev23.06.7"
config_vm_version_major              = "23"
config_vm_version_minor              = "06"
config_vm_version_patch              = "6"
config_vm_data_year                  = "23"
config_vm_data_month                 = "06"
config_vm_data_iteration             = "0"

// --- OpenAI API configuration --- //
config_openai_api_docker_image_version = "0.0.5"

// --- Web App configuration --- //
config_webapp_release = "v0.4.10"
// Data Context
config_webapp_data_context_release    = "23.06"


// --- Security Configuration --- //
// Enable / Disable network security policies application (Platform 'dev' default is false)
config_security_api_enable    = false
config_security_webapp_enable = false

