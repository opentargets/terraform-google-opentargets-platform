// --- Open Targets Platform PERMANENT development deployment ---//
// --- Release information --- //
config_release_name = "mbdevpf"

// --- Deployment configuration --- //
config_deployment_regions = ["europe-west1"]

// --- Elastic Search configuration --- //
config_vm_elastic_search_data_volume_image = "mbdevpos-20230506-1528-es"

// --- Clickhouse configuration --- //
config_vm_clickhouse_data_volume_image = "mbdevpos-20230506-1528-ch"

// --- API configuration --- //
config_vm_platform_api_image_version = "dev23.04.1"
config_vm_version_major              = "dev23"
config_vm_version_minor              = "04"
config_vm_version_patch              = "1"
config_vm_data_year                  = "23"
config_vm_data_month                 = "04"
